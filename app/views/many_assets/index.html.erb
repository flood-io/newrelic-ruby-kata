<h1>ManyAssets#index</h1>
<p>Find me in app/views/many_assets/index.html.erb</p>
<div class="well">
  <% @num_high.times do |i| -%>
    <% @num_wide.times do |j| -%>
      <% icon = @icons[i*@num_wide + j] -%>
      <%= image_tag 's.gif', title: icon.web_site.url, class: "ico#{icon.id}" %>
    <% end -%>
    <br>
  <% end %>
</div>